{
    //
    // Misc module configurations
    //
    "custom/sep": {
        "format": " ‚Åù‚Åù",
    },
    // house icon, starts the "menu"
    "custom/launcher": {
        "format": "ÔÄï",
        "on-click": "exec fuzzel",
        "tooltip": false
    },
    // window title
    "sway/window": {
        "format": "<b>{app_id}:</b> <i>{title}</i>",
        "max-length": 50,
        "tooltip": true,
    },
    "sway/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "format": "{name}",
        "format_focused": "< %g ‚óè >",
        // name : output (default all)
        "persistent-workspaces": {
            "1": [],
            "2": [],
            "3": [],
            "4": [],
            "5": [],
            "6": [],
        }
    },
    // display sway "mode" texts or controls e.g. ctrl-esc for exit
    "sway/mode": {
        "format": "<span style=\"italic\">{}</span>",
        "max-length": 50
    },
    // swap full config of waybar
    "custom/config-swap": {
        "on-click": "~/.config/waybar/scripts/swap.sh",
        "tooltip": "Swap between waybar configs",
        "format": "Bg ÔàÑ "
    },
    // runing task icons
    "wlr/taskbar": {
        "format": "{icon}",
        "sort-by-app-id": true,
        "icon-size": 13,
        "icon-theme": "Numix-Circle",
        "tooltip-format": "{title}",
        "on-click": "activate",
        "on-click-right": "close",
        "markup": true,
        "ignore-list": [
            "kitty"
        ],
    },
    // tray for bg programs
    "tray": {
        "icon-size": 14,
        "spacing": 5
    },
    // ???
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },
    // house icon, starts the "menu"
    "custom/launcher": {
        "format": "ÔÄï",
        "on-click": "exec fuzzel",
        "tooltip": false
    },
    // for waybar development - easier restart
    "custom/reload-config": {
        // you can start a custom command immediately after starting the waybar
        //"exec": "sh ~/.config/waybar/scripts/something.sh",
        "format": "Reload conf",
        "tooltip": false,
        "on-click": "swaymsg reload & pkill waybar & waybar",
        "layer": "below",
        "output": "all"
    },
    // open the notification panel
    "custom/notification": {
        "tooltip": true,
        "format": "{icon}",
        "format-icons": {
            "notification": "ÔÇ¢<span foreground='red'><small><sup>‚¨§</sup></small></span>",
            "none": "ÔÇ¢",
            "dnd-notification": "Ôá∑<span foreground='red'><small><sup>‚¨§</sup></small></span>",
            "dnd-none": "Ôá∑"
        },
        "return-type": "json",
        "exec-if": "which swaync-client",
        "exec": "swaync-client -swb",
        "on-click": "swaync-client -t -sw",
        "on-click-right": "swaync-client -d -sw",
        "escape": true
    },
    "user": {
        "format": "{user}",
        "exec": "whoami",
        "interval": 1
    },
    "custom/bluelightfilter": {
        "format": "üåÖ",
        "interval": 10,
        "return-type": "json",
        "exec": "~/.config/waybar/scripts/bluelightfilter-monitor.sh",
        "on-click": "~/.config/waybar/scripts/bluelightfilter-toggle.sh",
        "signal": 8
    },
    "network": {
        // "interface": "wlp2s0", // (Optional) To force the use of this interface
        "format": "‚ö†‚ÄâDisabled",
        "format-wifi": "Ôá´‚Äâ{essid}‚Äâ{signalStrength}%",
        // "format-ethernet": "ÔÉÅ {ifname}: {ipaddr}/{cidr}",
        "format-ethernet": "ÔÉÅ‚ÄâWired",
        "format-disconnected": "‚ö†‚ÄâOffline",
        "on-click-right": "alacritty -e nmtui",
        "on-click": "nm-connection-editor",
        "tooltip-format": "{ifname} via {gwaddr} :: {ipaddr}/{cidr} Û∞äó",
        "tooltip-format-wifi": "{essid} ({signalStrength}%) :: {ipaddr}/{cidr} Ôá´",
        "tooltip-format-ethernet": "{ifname} :: {ipaddr}/{cidr} ÔÉÅ",
        "tooltip-format-disconnected": "Disconnected",
    },
    "custom/keyboard-layout": {
        "exec": "swaymsg -t get_inputs | grep -m1 'xkb_active_layout_name' | cut -d '\"' -f4 | cut -b 1-2",
        // Interval set only as a fallback, as the value is updated by signal
        "interval": 1,
        "format": "ÔÑú‚Äâ{}", // Icon: keyboard
        // Signal sent by Sway key binding (~/.config/sway/key-bindings)
        "signal": 1, // SIGHUP
        "tooltip": false,
        "on-click": "swaymsg input 'type:keyboard' xkb_switch_layout next"
    },
    "custom/power": {
        "format": "‚èª",
        "on-click": "nwg-bar",
        "tooltip": false
    },
    // *** example of scheduled exec: ***
